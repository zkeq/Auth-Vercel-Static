{
    "version": 2,
    "public": true,
    "builds": [
      { "src": "Api-Auth-Vercel-Static/main.py", "use": "@vercel/python" }
    ],
    "routes": [
      { "src": "/.*", "dest": "Api-Auth-Vercel-Static/main.py" }
  ],
    "headers": [
      {
        "source": "/(.*)",
        "headers": [
          {
            "key": "Access-Control-Allow-Origin",
            "value": "*"
          },
          {
            "key": "Access-Control-Allow-Methods",
            "value": "*"
          },
          {
            "key": "Access-Control-Allow-Headers",
            "value": "*"
          }
        ]
      }
    ]
  }